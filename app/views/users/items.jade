#items-dashboard
  .clear
  //- each item, i in user.items
  //-   img.item(src=user.items[i].image)

  each item in user.items
    case item.type
      when 'autogrow'
        include ../items/autogrow
      when 'autoseed'
        include ../items/autoseed
      when 'autoroot'
        include ../items/autoroot

block scripts
  script(src='/js/vendor/jquery.nouislider.min.js')
  script(src='/js/es6/compiled/autogrow.js')
